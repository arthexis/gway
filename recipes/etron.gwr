# recipes/etron.gwr
# OCPP 1.6 CSMS Recipe with basic RFID auth

web app setup 
    --project web.cookies 
    --project web.navbar 
    --project vbox --home upload
    --project ocpp --home status
    
ocpp setup-csms-app --allowlist data/etron/rfids.cdv --location porsche_centre

web server start-app --host 0.0.0.0 --port 8000 --ws-port 9000 
until --lock-file VERSION
