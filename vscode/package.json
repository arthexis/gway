{
  "name": "gway-vscode",
  "displayName": "gway",
  "publisher": "Gelectriic Solutions",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:gway.runFile",
    "onCommand:gway.runSelection"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gway.runFile",
        "title": "gway: Run on Current File"
      },
      {
        "command": "gway.runSelection",
        "title": "gway: Run on Selection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.60.0",
    "@vscode/test-electron": "^2.0.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "images",
    "resources",
    "out",
    "package.json"
  ]
}
